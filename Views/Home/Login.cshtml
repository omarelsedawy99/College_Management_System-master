@{
    ViewBag.Title = "Login";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="~/Assets/css/icons/font-awesome/font-awesome.js"></script>

    <link rel="icon" type="image/png" href="~/Content/Home/images/icons/favicon.ico" />
    <link rel="stylesheet" type="text/css" href="~/Content/Home/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Home/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Home/vendor/animate/animate.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Home/vendor/css-hamburgers/hamburgers.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Home/vendor/select2/select2.min.css">

    <link rel="stylesheet" type="text/css" href="~/Content/Home/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Home/css/main.css">

</head>
<body>
    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100" style="padding: 10% 25% 10% 25%;">
                <div class="login100-pic js-tilt" data-tilt>
                    <img src="~/Content/Home/images/img-01.png" alt="IMG">
                </div>
                <form class="login100-form validate-form"  action="~/Home/Login" method="post">
                    <span class="login100-form-title" style="padding-top: 10%;padding-bottom: 15%;">
                    </span>
                    <div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
                        <input class="input100" type="text" name="Email" placeholder="Email">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fas fa-user-lock" aria-hidden="true"></i>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Password is required">
                        <input class="input100" type="password" name="Password" placeholder="Password">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fas fa-lock" aria-hidden="true"></i>
                        </span>
                    </div>
                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn" type="submit">
                            Login
                        </button>
                    </div>
                    <div class="text-center p-t-12">
                    </div>
                </form>
            </div>
        </div>
    </div>

    <script src="~/Content/Home/vendor/jquery/jquery-3.2.1.min.js" type="cbd5967b44bf9c3669c5c816-text/javascript"></script>

    <script src="~/Content/Home/vendor/bootstrap/js/popper.js" type="cbd5967b44bf9c3669c5c816-text/javascript"></script>
    <script src="~/Content/Home/vendor/bootstrap/js/bootstrap.min.js" type="cbd5967b44bf9c3669c5c816-text/javascript"></script>

    <script src="~/Content/Home/vendor/select2/select2.min.js" type="cbd5967b44bf9c3669c5c816-text/javascript"></script>

    <script src="~/Content/Home/vendor/tilt/tilt.jquery.min.js" type="cbd5967b44bf9c3669c5c816-text/javascript"></script>
    <script type="cbd5967b44bf9c3669c5c816-text/javascript">
        $('.js-tilt').tilt({
            scale: 1.1
        })
    </script>

    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-23581568-13" type="cbd5967b44bf9c3669c5c816-text/javascript"></script>
    <script type="cbd5967b44bf9c3669c5c816-text/javascript">
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-23581568-13');
    </script>

    <script src="~/Content/Home/js/main.js" type="cbd5967b44bf9c3669c5c816-text/javascript"></script>
    <script src="https://ajax.cloudflare.com/cdn-cgi/scripts/7089c43e/cloudflare-static/rocket-loader.min.js" data-cf-settings="cbd5967b44bf9c3669c5c816-|49" defer=""></script>
</body>
</html>
